--
-- Name: acs_get_user(text); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION acs_get_user(text) RETURNS text
    LANGUAGE plpgsql
    AS $_$
DECLARE
cnt int;
user_auth text;
BEGIN
user_auth = '';
SELECT user_id INTO user_auth FROM acs.tokens WHERE token=$1 AND exp_date >= now();

RETURN user_auth;
END;
$_$;


